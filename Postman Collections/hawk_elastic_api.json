				{
					"name": "Get All Indexes",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "4dc352c3-e9ab-4dea-9de1-199166e1440b",
								"exec": [
									"pm.test(\"Response to have indexes\", function () {",
									"    pm.response.to.have.status(200);",
									"    ",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.be.an(\"object\");",
									"    pm.expect(jsonData.IndexNames).to.have.lengthOf.at.least(1);",
									"    ",
									"});",
									"",
									"const jsonData = pm.response.json();",
									"let nonCurrentIndex;",
									"for (let i = 0; i < jsonData.IndexNames.length; i++) {",
									"    if (jsonData.IndexNames[i] === pm.variables.get(\"current_index_name\")) {",
									"        continue;",
									"    }",
									"    pm.environment.set(\"index_name\", jsonData.IndexNames[i]);",
									"}",
									"",
									""
								],
								"type": "text/javascript"
							}
						}
